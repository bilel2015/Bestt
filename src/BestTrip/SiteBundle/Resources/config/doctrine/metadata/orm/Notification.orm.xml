<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Notification" table="Notification">
        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="description" type="string" column="description" length="255" nullable="true"/>
        <field name="dateCreation" type="string" column="date_creation" length="255" nullable="true"/>
        <field name="entite" type="string" column="entite" length="255" nullable="true"/>
        <field name="idEntite" type="integer" column="id_entite" nullable="true"/>
        <field name="idUser" type="integer" column="id_user" nullable="true"/>
        <field name="traite" type="boolean" column="traite" nullable="true"/>
    </entity>
</doctrine-mapping>
